hello.conf

pstream hello {
    server hello.default.svc.cluster.local;
}
server {
    listen 443;
    ssl    on;
    ssl_certificate     /home/prabha_techsub/tls/cert.pem;
    ssl_certificate_key /home/prabha_techsub/tls/key.pem;
    location / {
        proxy_pass http://helloworld;
    }
}

